
subproof PSA(int N = 2**11, int chunks = 16, int loops = 3) {
    container subproof.ps {
        int k = 100;
    }
    col witness w;
    int values[12][20];
    subproof.ps.k = subproof.ps.k + 1;
    println("PBA", N, chunks, loops, subproof.ps.k);
}

