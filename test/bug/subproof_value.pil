air AirA(int N = 2**16, int type = 0) {
    col witness a,b,c,d;

    subproofval aggregate(sum) typeg;
    if (type == 0) {
        subproofval aggregate(prod) type0;
        type0 === a * b;
        typeg === a * c;
    } else {
        subproofval aggregate(prod) type0;
        type0 === c * b;
        typeg === c * c;
    }
}

air AirB(int N = 2**16) {
    col witness a,b;

    subproofval aggregate(sum) typeb;
    typeb === a * b;
}

AirA(2**8);
AirA(2**16);
AirB(2**8);
